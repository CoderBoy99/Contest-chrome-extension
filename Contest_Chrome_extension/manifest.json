{
  "manifest_version": 2,
  "name": "Contests",
  "version": "1.0",
  "description": "Provides info for lastest contest on Codechef, Codeforces & Leetcode",
  "background" : {
  "scripts" : ["background.js"],
  "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":[
      "notifications"
  ] 
}